MACRO kick
	attack = 0.01
	wave = 4 0
	puslewidth = 0.5
	offset = 60 10 6 0 -4 -5
	volume = 1 1 1 | +-0.3


MACRO snare
	attack = 0.01
	wave = 4 0 0 4
	puslewidth = 0.5
	offset = 40 12 0 70
	volume = 1 1 1 | +-0.08

MACRO bass
	panning = -0.1
	wave = 0
	pulsewidth = 0.5 0.4 0.3 0.2
	volume = 1 1 1 1 0.8 0.7 0.6
