frames = 800
ticks = 8

MACRO kick
	wave = 4 0
	puslewidth = 0.5
	offset = 60 10 6 0 -4 -5
	volume = 1 1 1 1 | +-0.3

MACRO snare
	wave = 4 0 0 0 4
	puslewidth = 0.5
	offset = 40 12 4 0 70
	volume = 1 1 1 1 1 | +-0.08


MACRO bass
	panning = -0.1
	wave = 0
	puslewidth = 0.2 0.3 0.4 0.5
	offset = 7 0
	volume = 1 1 1 0.8 0.6 0.4 0.2


MACRO pd
	panning = -0.2 | +0.02 +0.3 +-0.3
	wave = 1
	resolution = 20
	pulsewidth = 0.01 | +0.02
	release	= 0.9999
	decay	= 0.9999
	attack = 0.001

MACRO pd037 < pd
	offset = | 0 0 3 3 7 7 12 12

MACRO pd047 < pd
	panning = 0.3 | +-0.02 +-0.3 +0.3
	offset = | 0 0 4 4 7 7 12 12


MACRO lead
	offset = 0
	panning = 0.1
	volume = 1
	wave = 3 0
	pulsewidth = 0.3
	vibratodepth = 0 0 0 0 0 0 0.1 0.2 0.3
	vibratospeed = 0.1

MACRO midi < lead

MACRO fx
	wave = | 4 4 3
	offset = 0 | +12 +0 +-12.2
	panning = 0.5 | +-0.01
	volume = 1 | +-0.01
